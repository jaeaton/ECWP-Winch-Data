<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="550"
        x:Class="Views.MainWindow"
		xmlns:controls="clr-namespace:Views"
        Title="ECWP Winch Data Collection">

	<TabControl>
		<TabItem Header="Live Data" VerticalContentAlignment="Center">
			<controls:PlottingView/>
		</TabItem>
		<TabItem Header="Cast Processing" VerticalContentAlignment="Center">
			<controls:ProcessingView/>
		</TabItem>
		<TabItem Header="Winch Config" VerticalContentAlignment="Center">
			<controls:WinchConfigurationView />
		</TabItem>
		<TabItem Header="Update Check" VerticalContentAlignment="Center">
			<controls:UpdateCheckView />
		</TabItem>
	</TabControl>
</Window>
